<MOPExamples>
        <MOP name="Binh (1) (Binh and Korn 1996)">
             <Function value="x**2 + y**2"/>
             <Function value="(x - 5)**2 + (y - 5)**2"/>
             <Variable value="x" lower_range="-5" upper_range="10"/>
             <Variable value="y" lower_range="-5" upper_range="10"/>            
        </MOP>

        <MOP name="Kursawe (1) (Kursawe 1991)">
             <Function value="-10*e**((-0.2)*(x**2 + y**2)**(1.0/2.0)) - 10*e**((-0.2)*(y**2 + z**2)**(1.0/2.0))"/>
             <Function value="abs(x)**0.8 + 5*sin(x**3) + abs(y)**0.8 + 5*sin(y**3) + abs(z)**0.8 + 5*sin(z**3)"/>
             <Variable value="x" lower_range="-5" upper_range="5"/>
             <Variable value="y" lower_range="-5" upper_range="5"/>
             <Variable value="z" lower_range="-5" upper_range="5"/>                        
        </MOP>

        <MOP name="Fonseca (1) (Fonseca and Flemming 1995d)">
             <Function value="1 - e**(-1 * (x - 1)**2 - (y + 1)**2)"/>
             <Function value="1 - e**(-1 * (x + 1)**2 - (y - 1)**2)"/>
             <Variable value="x" lower_range="-4" upper_range="4"/>
             <Variable value="y" lower_range="-4" upper_range="4"/>            
        </MOP>

        <MOP name="Viennet (3) (Viennet et al. 1996)">
             <Function value="0.5*(x**2 + y**2) + sin(x**2 + y**2)"/>
             <Function value="((3*x - 2*y + 4)**2)/8 + ((x - y + 1)**2)/27 + 15"/>
             <Function value="1/(x**2 + y**2 + 1) - 1.1*e**(-x**2 - y**2)"/>
             <Variable value="x" lower_range="-3" upper_range="3"/>
             <Variable value="y" lower_range="-3" upper_range="3"/>            
        </MOP>
          
</MOPExamples>
